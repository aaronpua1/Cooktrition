	</div>
	<div id="page-wrapper">
		<div class="row">
			<div class="col-lg-12">
				<h1 class="page-header">Recommended Recipes</h1>
				{{#each results}}	
					<section class="col-xs-12 col-sm-6 col-md-12">
						<article class="search-result row">
							<div class="col-xs-12 col-sm-12 col-md-3">
								<a href="/recipe/recipeinfo?id={{this.id}}" title="{{this.title}}" class="thumbnail" id="{{this.id}}"><img src={{this.image}} alt="" /></a>
							</div>
							<div class="col-xs-12 col-sm-12 col-md-2">
								<p>Calories: {{this.calories}}</p>
								<p>Total Fat: {{this.fat}}</p>		
								<p>Carbohydrate: {{this.carbs}}</p>	
								<p>Protein: {{this.protein}}</p>
							</div>
							<div class="col-xs-12 col-sm-12 col-md-7 excerpet">
								<h3><a href="/recipe/recipeinfo?id={{this.id}}" title="">{{this.title}}</a></h3>		
							</div>
							<span class="clearfix borda"></span>
						</article>
					</section>
				{{/each}}
			</div>
		</div>           
	</div>
</div> 